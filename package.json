{
  "name": "vue_pro",
  "version": "1.0.2",
  "private": true,
  "main": "main.js",
  "description": "An exquisite local music player",
  "author": "Violex",
  "productName": "Sonorbit",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "appId": "Sonorbit",
    "win": {
      "icon": "/dist/img/logo.ico"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "node_modules/**"
    ],
    "directories": {
      "output": "release"
    },
    "asar": false
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bean": "latest",
    "core-js": "^3.8.3",
    "drag": "^0.0.4",
    "electron-drag": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.1.0",
    "electron-win-state": "^1.1.22",
    "electron-window-state": "^5.0.3",
    "fs": "^0.0.1-security",
    "howler": "^2.2.3",
    "jsmediatags": "^3.9.7",
    "mime-types": "^2.1.35",
    "music-metadata": "^8.1.4",
    "NeteaseCloudMusicApi": "^4.15.3",
    "node-id3": "^0.2.6",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "qs": "^6.11.2",
    "sharp": "^0.32.6",
    "vue": "^2.6.14",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^3.6.5",
    "vue-slider-component": "^3.2.24",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^19.0.8",
    "electron-builder": "^24.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
